.menu-bar
  transition: 0.5s ease
  display: flex
  flex-direction: column
  padding: 20px
  padding-left: 25px
  height: 100vh
  box-sizing: border-box
  width: 300px
  position: absolute
  top: 0
  border-right: 2px solid black
  background: white
  z-index: 4
  label
    display: inline-block
  h3
    text-align: right
    letter-spacing: 5px
  .block-links
    padding: 40px
    padding-top: 65px
  .resume-block
    margin: 30px 0